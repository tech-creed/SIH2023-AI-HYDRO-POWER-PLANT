<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title>Image and Prompt to AI 2D Image Generation</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta content="" name="keywords" />
    <meta content="" name="description" />

    <!-- Favicon -->
    <link href="img/favicon.ico" rel="icon" />

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;500;600&family=Teko:wght@400;500;600&display=swap"
      rel="stylesheet"
    />
    <!-- Icon Font Stylesheet -->
    <link
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css"
      rel="stylesheet"
    />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css"
      rel="stylesheet"
    />

    <!-- Libraries Stylesheet -->
    <link href="lib/animate/animate.min.css" rel="stylesheet" />
    <link href="lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet" />
    <link
      href="lib/tempusdominus/css/tempusdominus-bootstrap-4.min.css"
      rel="stylesheet"
    />

    <link href="css/bootstrap.min.css" rel="stylesheet" />

    <link href="css/style.css" rel="stylesheet" />

    <style>
      .stretch {
        display: flex;
        justify-content: center;
        margin-bottom: 20px;
      }

      .form-container {
        flex: 1;
        position: relative;
      }

      .text-area {
        width: 100%;
        padding: 10px;
        border: 2px solid #ccc;
        border-radius: 8px;
        font-size: 16px;
        resize: none;
        outline: none;
        height: auto;
        min-height: 50px;
        max-height: 150px;
        overflow-y: auto;
        scrollbar-width: thin;
        scrollbar-color: #b78d65 #fff;
      }

      .text-area::-webkit-scrollbar {
        width: 8px;
      }

      .text-area::-webkit-scrollbar-thumb {
        background-color: #b78d65;
        border-radius: 4px;
      }

      .text-area::-webkit-scrollbar-track {
        background-color: #fff;
      }

      .send-button {
        position: absolute;
        right: 15px;
        bottom: 10px;
        padding: 8px;
        background-color: #b78d65;
        color: white;
        border: none;
        border-radius: 5px;
        cursor: pointer;
      }

      .send-button:focus {
        outline: none;
      }

      .send-button svg {
        stroke: white;
      }

      .generated-images {
        display: flex;
        flex-wrap: wrap;
        justify-content: center;
        margin-top: 20px;
      }

      .generated-image {
        margin: 10px;
        border-radius: 8px;
        overflow: hidden;
        cursor: pointer;
      }

      .generated-image img {
        width: 200px;
        height: 200px;
        object-fit: cover;
      }

      .input-container {
        text-align: center;
        margin-bottom: 20px;
      }

      #imageInput {
        display: none;
      }

      .custom-file-upload {
        display: inline-block;
        padding: 10px 15px;
        cursor: pointer;
        background-color: #4caf50;
        color: white;
        border-radius: 5px;
      }

      /* Styles for medium image container */
      .medium-image-container {
        text-align: center;
        margin-bottom: 20px;
      }

      .btn-center-align {
        display: flex;
        justify-content: center;
        margin-top: 10px;
      }
      .medium-image-container img {
        max-width: 50%;
        height: auto;
        cursor: pointer;
        border: 1px solid #ccc;
        border-radius: 5px;
      }

      /* Styles for lightbox */
      .lightbox {
        display: none;
        position: fixed;
        z-index: 9999;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background-color: rgba(0, 0, 0, 0.9);
        text-align: center;
      }

      .popupCanvas {
        display: none;
      }

      .lightbox .close-btn {
        color: #fff;
        font-size: 40px;
        cursor: pointer;
        position: absolute;
        top: 20px;
        right: 30px;
      }

      .close-btn {
        color: white;
        font-size: 20px;
        position: absolute;
        top: 20px;
        right: 30px;
        cursor: pointer;
      }
      .reset-btn{
        color: #fff;
        text-decoration: none;
        font-size: 24px;
        position: absolute;
        top: 32px;
        right: calc(30px + 40px);
        cursor: pointer;
      }

      .lightbox .lightbox-content {
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
      }

      /* gen image popup */

      .generated-lightbox {
        display: none;
        position: fixed;
        z-index: 9999;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background-color: rgba(0, 0, 0, 0.9);
        text-align: center;
      }

      .popupGeneratedCanvas {
        display: none;
      }

      .generated-lightbox .generated-close-btn {
        color: #fff;
        font-size: 40px;
        cursor: pointer;
        position: absolute;
        top: 20px;
        right: 30px;
      }

      .generated-close-btn,
      .generated-reset-btn {
        color: white;
        font-size: 20px;
        position: absolute;
        top: 20px;
        right: 30px;
        cursor: pointer;
      }

      .generated-lightbox .generated-lightbox-content {
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
      }

      .generated-lightbox .generated-download-btn {
        color: #fff;
        text-decoration: none;
        font-size: 24px;
        position: absolute;
        top: 32px;
        right: calc(30px + 40px);
        cursor: pointer;
      }

      .generated-lightbox .generated-download-btn i {
        margin-right: 5px;
      }

      .medium-image-container {
        display: none;
      }
    </style>
  </head>
  <body>
    <!-- Spinner Start -->
    <div
      id="spinner"
      class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center"
    >
      <div
        class="spinner-border position-relative text-primary"
        style="width: 6rem; height: 6rem"
        role="status"
      ></div>
      <img
        class="position-absolute top-50 start-50 translate-middle"
        src="img/icons/icon-1.png"
        alt="Icon"
      />
    </div>
    <!-- Spinner End -->

    <!-- Navbar Start -->
    <nav
      class="navbar navbar-expand-lg bg-white navbar-light sticky-top py-lg-0 px-lg-5 wow fadeIn"
      data-wow-delay="0.1s"
    >
      <a href="index.html" class="navbar-brand ms-4 ms-lg-0">
        <h1 class="text-primary m-0">
          <img class="me-3" src="img/icons/icon-1.png" alt="Icon" />HydroCraft
        </h1>
      </a>
      <button
        type="button"
        class="navbar-toggler me-4"
        data-bs-toggle="collapse"
        data-bs-target="#navbarCollapse"
      >
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarCollapse">
        <div class="navbar-nav ms-auto p-4 p-lg-0">
          <a href="/" class="nav-item nav-link active">Home</a>
        </div>
        <a href="" class="btn btn-primary py-2 px-4 d-none d-lg-block"
          >Features</a
        >
      </div>
    </nav>
    <!-- Navbar End -->

    <!-- Service Start -->
    <div class="container-xxl py-5">
      <div class="container mt-5">
        <h2>Prompt & Image to AI Generate 2D Hydro Power Plant</h2>

        <div class="generated-images" id="generatedImages">
          <!-- images will be populated here -->
        </div>

        <br />
        <br />

        <div id="generatedLightbox" class="generated-lightbox" onclick="closeGeneratedLightbox()">
          <div class="generated-button-container">
            <span class="generated-close-btn" onclick="closeGeneratedLightbox()">&times;</span>
            <a
              id="generatedDownloadLink"
              href="#"
              download="generated_image.jpg"
              class="generated-download-btn"
            >
              <i class="fa fa-download"></i>
            </a>
          </div>
          <div class="generated-lightbox-content">
            <img id="generatedImage" src="" alt="Generated Image" />
          </div>
        </div>
        

        <!-- Form for prompt input -->
        <form class="stretch" id="promptAndImageForm">
          <div class="form-container">
            <textarea
              id="prompt-textarea"
              tabindex="0"
              rows="1"
              placeholder="Enter Your Prompt..."
              class="text-area"
              required
            ></textarea>
            <button type="submit" class="send-button" data-testid="send-button">
              <span class="" data-state="closed">
                <svg width="24" height="24" viewBox="0 0 24 24" fill="none">
                  <path
                    d="M7 11L12 6L17 11M12 18V7"
                    stroke="white"
                    stroke-width="2"
                    stroke-linecap="round"
                    stroke-linejoin="round"
                  ></path>
                </svg>
              </span>
            </button>
          </div>
        </form>

        <div class="input-container">
          <input type="file" id="imageInput" accept="image/*" />
          <label for="imageInput" class="custom-file-upload"
            >Upload Image</label
          >
        </div>

        <div class="medium-image-container">
          <img
            id="inputImage"
            src="#"
            alt="Uploaded Image"
            onclick="openLightbox(this.src)"
          />
          <h5 style="color: #4caf50; padding-top: 3%">
            Click on the image to paint a specific area for generating the
            output image.
          </h5>

          <br />
          <div class="btn-center-align">
            <button
              onclick="clearUploadedImageAndCanvas()"
              class="btn btn-danger py-2 px-4 d-none d-lg-block"
            >
              Reset
            </button>
          </div>
        </div>

        <div id="lightbox" class="lightbox">
          <span class="close-btn" onclick="closeLightbox()">&times;</span>
          <span class="reset-btn" onclick="resetCanvas()"><i class="fas fa-undo"></i></span>
          <img
            style="display: none"
            id="popUpImage"
            class="lightbox-content"
            src="#"
            alt="Pop-up Image"
          />
          <div class="lightbox-content">
            <canvas id="popUpCanvas" width="600" height="400"></canvas>
          </div>
        </div>
      </div>
    </div>

    <!-- Footer Start -->
    <div
      class="container-fluid bg-dark text-body footer mt-5 pt-5 px-0 wow fadeIn"
      data-wow-delay="0.1s"
    >
      <div class="container py-5">
        <div class="row g-5">
          <div class="col-lg-3 col-md-6">
            <h3 class="text-light mb-4">Address</h3>
            <p class="mb-2">
              <i class="fa fa-map-marker-alt text-primary me-3"></i>Theni,
              Tamilnadu, India
            </p>
            <p class="mb-2">
              <i class="fa fa-phone-alt text-primary me-3"></i>+91 9876543210
            </p>
            <p class="mb-2">
              <i class="fa fa-envelope text-primary me-3"></i>info@techcreed.com
            </p>
            <div class="d-flex pt-2">
              <a class="btn btn-square btn-outline-body me-1" href=""
                ><i class="fab fa-twitter"></i
              ></a>
              <a class="btn btn-square btn-outline-body me-1" href=""
                ><i class="fab fa-facebook-f"></i
              ></a>
              <a class="btn btn-square btn-outline-body me-1" href=""
                ><i class="fab fa-youtube"></i
              ></a>
              <a class="btn btn-square btn-outline-body me-0" href=""
                ><i class="fab fa-linkedin-in"></i
              ></a>
            </div>
          </div>
          <div class="col-lg-3 col-md-6">
            <h3 class="text-light mb-4">Features</h3>
            <a class="btn btn-link" href="">RiverForecast</a>
            <a class="btn btn-link" href="">Rainfall Analysis</a>
            <a class="btn btn-link" href="">HydroVision</a>
            <a class="btn btn-link" href="">Metadata Predictive Analysis</a>
            <a class="btn btn-link" href="">2D-AI Image Generation</a>
          </div>
          <div class="col-lg-3 col-md-6">
            <h3 class="text-light mb-4">Quick Links</h3>
            <a class="btn btn-link" href="">About Us</a>
            <a class="btn btn-link" href="">Contact Us</a>
            <a class="btn btn-link" href="">Our Features</a>
            <a class="btn btn-link" href="">Terms & Condition</a>
            <a class="btn btn-link" href="">Support</a>
          </div>
          <div class="col-lg-3 col-md-6">
            <h3 class="text-light mb-4">Newsletter</h3>
            <p>
              Quick updates, no fuss. Subscribe to our newsletter for simplicity
              in your inbox.
            </p>
            <div class="position-relative mx-auto" style="max-width: 400px">
              <input
                class="form-control bg-transparent w-100 py-3 ps-4 pe-5"
                type="text"
                placeholder="Your email"
              />
              <button
                type="button"
                class="btn btn-primary py-2 position-absolute top-0 end-0 mt-2 me-2"
              >
                SignUp
              </button>
            </div>
          </div>
        </div>
      </div>
      <div class="container-fluid copyright">
        <div class="container">
          <div class="row">
            <div class="col-md-6 text-center text-md-start mb-3 mb-md-0">
              &copy; <a href="#">Hydro Craft</a>, All Right Reserved.
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Back to Top -->
    <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"
      ><i class="bi bi-arrow-up"></i
    ></a>

    <script>
      const textarea = document.getElementById("prompt-textarea");

      textarea.addEventListener("input", function () {
        this.style.height = "auto";
        this.style.height = this.scrollHeight + "px";
      });

      const promptForm = document.getElementById("promptAndImageForm");

      textarea.addEventListener("keydown", function (event) {
        if (event.keyCode === 13 && !event.shiftKey) {
          event.preventDefault();

          promptForm.dispatchEvent(new Event("submit"));
        }
      });
    </script>

    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="lib/wow/wow.min.js"></script>
    <script src="lib/easing/easing.min.js"></script>
    <script src="lib/waypoints/waypoints.min.js"></script>
    <script src="lib/counterup/counterup.min.js"></script>
    <script src="lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="lib/tempusdominus/js/moment.min.js"></script>
    <script src="lib/tempusdominus/js/moment-timezone.min.js"></script>
    <script src="lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>

    <!-- Template Javascript -->
    <script src="js/main.js"></script>

    <script src="js/maskImage.js"></script>
  </body>
</html>
